<Window x:Class="Calendar.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Calendar"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0*"/>
            <RowDefinition/>
            <RowDefinition Height="13*"/>
        </Grid.RowDefinitions>
        <Button Content="Register" HorizontalAlignment="Center" Margin="0,103,0,0" Grid.Row="2" VerticalAlignment="Top" Height="56" Width="136" Click="Button_Click" FontSize="24"/>
        <Button Content="Authorize" HorizontalAlignment="Center" Margin="0,186,0,0" Grid.Row="2" VerticalAlignment="Top" Height="62" Width="136" FontSize="24"/>
        <Label Content="Calendar" HorizontalAlignment="Center" Margin="0,10,0,0" Grid.Row="2" VerticalAlignment="Top" FontSize="48"/>
    </Grid>
</Window>
